{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "cargo build --bin 10k-dependency-graph",
            "group": "build",
            "problemMatcher": ["$rustc"]
        },
        {
            "label": "run",
            "type": "shell",
            "command": "cargo run --bin 10k-dependency-graph",
            "group": "test",
            "problemMatcher": ["$rustc"]
        }
    ]
}
